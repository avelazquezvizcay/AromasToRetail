<style>
  .product_tab_wrapper ul.tabs li.active>a,
  .product_tab_wrapper ul.tabs li>a:focus {
    background:{{ section.settings.tab_bg_color }};
    color:{{ section.settings.tab_text_color }};
  }
  .product_tab_wrapper ul.tabs li>a:hover {
    color:{{ section.settings.tab_text_color }};
  }
  .product_tab_wrapper ul.tabs li>a::after {
    background:{{ section.settings.tab_bg_color }};
  }
  share-button.share-button {
    float: left;
    width: 100%;
    margin: 0 0 15px;
  }
</style>
{% if template.name == 'product' %}
  <script src="{{ 'threesixty.min.js' | asset_url}}" defer="defer"></script>
  <script src="{{ 'jquery.zoom.min.js' | asset_url}}" defer="defer"></script>
{% endif %}
{% assign sold_out = true %}
{% if product.available %}
  {% assign sold_out = false %}
{% endif %}
{%- assign current_variant = product.selected_or_first_available_variant -%}
{%- assign product_image_zoom_size = '870x1131' -%}
{%- assign product_image_scale = '2' -%}
{%- assign enable_image_zoom = section.settings.product_image_zoom_enable -%}
{%- assign compare_at_price = current_variant.compare_at_price -%}
{%- assign price = current_variant.price -%}
<div
  itemscope
  itemtype="http://schema.org/Product"
  class="product-template__container page-width"
  id="ProductSection-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="product-template"
  data-enable-history-state="true"
  data-zoom-enable="{{ section.settings.product_image_zoom_enable }}"
  data-ajax-enabled="{{ settings.use_ajax_cart }}"
>
  <div class="container">
    <div class="product-single {{ section.settings.thumb_position }} {{ section.settings.product_layout }}">
      <div class="row">
        <div
          class="{% if section.settings.product_layout != 'design_3' %}col-xs-12 col-sm-5 {% endif %}demo text-center product-wrapper-owlslider"
          data-id="{{ product.id }}"
        >
          {% if product.images.size == 0 %}
            <div class="product-photo-container">
              <img src="{{ '' | product_img_url: 'grande' }}">
            </div>
          {% else %}
            {% if section.settings.product_layout == 'design_3' %}
              <div class="product-block demo">
                <div class="product-img owl-carousel owl-theme full_gallery_slider">
                  {%- for media in product.media -%}
                    {% case media.media_type %}
                      {% when 'image' %}
                        <div
                          class="pro_img image"
                          data-src="{{ media.src | img_url: 'master' }}"
                          data-position="{{ media.position }}"
                        >
                          <a href="{{ media.src | img_url: 'master' }}" class="product-single__thumb">
                            <img
                              height="auto"
                              id="productPhotoImg"
                              src="{{ media.src | img_url: section.settings.pro_img_widthheight  }}"
                              alt="{{ media.alt | escape }}"
                            >
                            <div class="num"></div>
                          </a>
                        </div>
                      {% when 'external_video' %}
                        {% assign external_video = media | external_video_tag %}
                        <div class="pro_img extra-video">
                          {{ external_video }}
                          <div class="num"></div>
                        </div>
                      {% when 'video' %}
                        <div class="pro_img video">
                          <div class="video-source" data-id="img_{{ media.id }}">
                            {% assign video = media
                              | video_tag: class: 'media-video', image_size: '1024x', autoplay: true
                            %}
                            {{ video }}
                          </div>
                          <div class="num"></div>
                        </div>
                      {% when 'model' %}
                        <div class="pro_img model">
                          <div class="model-view" data-id="img_{{ media.id }}">
                            {% assign model_view = media | model_viewer_tag %}
                            {{ model_view }}
                          </div>
                          <div class="num"></div>
                        </div>
                    {% endcase %}
                  {%- endfor -%}
                </div>
              </div>
            {% endif %}

            {% if section.settings.product_layout == 'design_1' %}
              <div class="product-single__photos" id="productPhoto">
                {% assign featured_media = product.selected_or_first_available_variant.featured_media
                  | default: product.featured_media
                %}
                {%- assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' -%}
                {% for media in product.media %}
                  <div
                    data-product-single-media-wrapper
                    class="product-single__media-wrapper{% if section.settings.product_layout == 'design_1' %}{% unless featured_media == media %} hide {% endunless %}{% endif %}{% if media.media_type != 'image' %} product__media-item--full{% endif %}{% if section.settings.hide_variants and variant_images contains media.src %} product__media-item--variant{% endif %}"
                    data-media-id="{{ section.id }}-{{ media.id }}"
                    tabindex="-1"
                  >
                    {% case media.media_type %}
                      {% when 'image' %}
                        {%- assign img_url = media | img_url: section.settings.pro_img_widthheight -%}
                        <div
                          class="product__media media{% if enable_image_zoom %} js-zoom-enabled{% endif %}"
                          {% if enable_image_zoom %}
                            data-image-zoom-wrapper
                            data-zoom="{{ media | img_url: product_image_zoom_size, scale: image_scale }}"
                          {% endif %}
                        >
                          <img
                            id="productPhotoImg"
                            src="{{ img_url }}"
                            {% if enable_image_zoom %}
                              data-image-zoom
                            {% endif %}
                          >
                        </div>
                      {% when 'external_video' %}
                        <div class="product__media media">
                          {{ media | external_video_tag }}
                        </div>
                      {% when 'video' %}
                        <div class="product__media media">
                          {{
                            media
                            | video_tag: class: 'media-video', image_size: image_size, controls: true, autoplay: true
                          }}
                        </div>
                      {% when 'model' %}
                        <div class="product__media media" style="padding-top: 100%">
                          {{
                            media
                            | model_viewer_tag:
                              image_size: image_size,
                              reveal: 'interaction',
                              toggleable: true,
                              data-model-id: media.id
                          }}
                        </div>
                      {% else %}
                        <div
                          class="product__media media"
                          style="padding-top: {{ 1 | divided_by: media.aspect_ratio | times: 100 }}%;"
                        >
                          {{ media | media_tag: class: 'media-item', image_size: image_size }}
                        </div>
                    {% endcase %}
                  </div>
                {% endfor %}
                <div class="product-image-btn">
                  <div class="gallery">
                    {% for image in product.images %}
                      {% if featured_image != image.src %}
                        <a
                          class="product-lightbox-btn tip-top img_{{ image.id }}"
                          data-id="img_{{ image.id }}"
                          href="{{ image.src | img_url: 'master' }}"
                        >
                          <i class="mdi mdi-magnify"></i
                        ></a>
                      {% endif %}
                    {% endfor %}
                    <a
                      class="product-lightbox-btn tip-top img_{{ featured_image.id }}"
                      data-id="img_{{ featured_image.id }}"
                      data-tip="Zoom"
                      href="{{ featured_image | img_url: 'master' }}"
                      ><i class="mdi mdi-magnify"></i
                    ></a>
                  </div>
                </div>
              </div>
              {% assign first_3d_model = product.media | where: 'media_type', 'model' | first %}
              {%- if first_3d_model -%}
                <button
                  aria-label="{{ 'products.product.view_in_space_label' | t }}"
                  class="product-single__view-in-space"
                  data-shopify-xr
                  data-shopify-model3d-id="{{ first_3d_model.id }}"
                  data-shopify-title="{{ product.title | escape }}"
                  data-shopify-xr-hidden
                >
                  <span class="product-single__view-in-space-text">{{ 'products.product.view_in_space' | t }}</span>
                </button>
              {%- endif -%}
              {% if product.images.size >= 1 %}
                <product-thumb>
                  <ul
                    class="product-single__thumbs product-single__thumbnails-{{ section.id }} {{ section.settings.thumb_position }}"
                    id="productThumbs"
                  >
                    {% for media in product.media %}
                      <li class="grid-item">
                        <a
                          href="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"
                          class="product-single__thumbnail--{{ section.id }} text-link product-single__thumbnail {% if media.media_type == 'video' %} video{% endif %}{% if media.media_type == 'external_video' %} extra-video{% endif %}{% if media.media_type == 'model' %} model{% endif %}"
                          data-thumbnail-id="{{ section.id }}-{{ media.id }}"
                          {% if product_image_zoom_enable %}
                            data-zoom="{{ media.preview_image | img_url: product_image_zoom_size, scale: product_image_scale }}"
                          {% endif %}
                        >
                          <img
                            class="product-single__thumbnail-image"
                            src="{{ media.preview_image | img_url: section.settings.pro_thumb_img_widthheight }}"
                            alt="{{ thumbnailAlt }}"
                          >
                        </a>
                      </li>
                    {% endfor %}
                  </ul>
                </product-thumb>
              {% endif %}
            {% endif %}

            {% if section.settings.product_layout == 'design_2' %}
              {% if product.images.size >= 1 %}
                <div class="product-block">
                  {%- for media in product.media -%}
                    <div class="product-single__media-wrapper" data-media-id="{{ section.id }}-{{ media.id }}">
                      {% case media.media_type %}
                        {% when 'image' %}
                          <div
                            class="pro_img{% if media == featured_image %} variant-active{% endif %}"
                            data-image-id="{{ media.id }}"
                            data-src="{{ media.src | img_url: 'master' }}"
                          >
                            <div class="product-single__photo-wrapper">
                              <div class="product-single__thumb">
                                <img
                                  class="product-single__photo"
                                  id="productPhotoImg"
                                  src="{{ media.src | img_url: section.settings.pro_img_widthheight }}"
                                  {% if settings.product_zoom_enable %}
                                    data-mfp-src="{{ media.src | img_url: '1024x1024' }}"
                                  {% endif %}
                                  alt="{{ media.alt | escape }}"
                                  data-image-id="{{ media.id }}"
                                >
                              </div>
                            </div>
                            <div class="product-image-btn">
                              <a
                                class="product-lightbox-btn tip-top"
                                data-tip="Zoom"
                                href="{{ media.src | img_url: 'master' }}"
                                ><i class="mdi mdi-magnify"></i
                              ></a>
                            </div>
                          </div>
                        {% when 'external_video' %}
                          {% assign external_video = media | external_video_tag %}
                          <div class="pro_img" data-image-id="{{ media.id }}">
                            {{ external_video }}
                          </div>
                        {% when 'video' %}
                          <div class="pro_img" data-image-id="{{ media.id }}">
                            <div class="video-source" data-id="img_{{ media.id }}">
                              {% assign video = media
                                | video_tag: class: 'media-video', image_size: '1024x', autoplay: true
                              %}
                              {{ video }}
                            </div>
                          </div>
                        {% when 'model' %}
                          <div class="pro_img" data-image-id="{{ media.id }}">
                            <div class="model-view" data-id="img_{{ media.id }}">
                              {% assign model_view = media | model_viewer_tag %}
                              {{ model_view }}
                            </div>
                          </div>
                      {% endcase %}
                    </div>
                  {%- endfor -%}
                </div>
              {% endif %}
            {% endif %}
          {% endif %}
        </div>
        <div class="{% if section.settings.product_layout == 'design_3' %}col-sm-12 col-xs-12 {% endif %}{% if section.settings.product_layout != 'design_3' %}col-xs-12 col-sm-7 {% endif %}demo product-information product_fixed_block">
          <div data-id="product-{{ product.id }}" class="product-information-inner product-block fixed-product-block">
            {% if section.settings.product_layout == 'design_3' %}
              <div class="container">
            {% endif %}
            {% if section.settings.product_layout != 'design_3' %}
              <div class="fixed-content">
                <div class="product-single__header col-xs-12">
            {% endif %}
            {% if section.settings.product_layout == 'design_3' %}
              <div class="product-single_wrap col-xs-12">
            {% endif %}
            <div class="title-arrows">
              <h1
                itemprop="name"
                class="product-single__title text-capitalize{% if section.settings.product_vendor_enable %} wvendor{% endif %}"
              >
                {{ product.title }}
              </h1>
              {% if collection %}
                {% if collection.previous_product or collection.next_product %}
                  <p class="product-arrows">
                    {% if collection.previous_product %}
                      {% capture prev_url %}{{ collection.previous_product}}#content{% endcapture %}
                      <span class="left">
                        <a href="{{ prev_url }}"> <i class="mdi mdi-chevron-left"></i></a>
                      </span>
                    {% endif %}
                    {% if collection.next_product %}
                      {% capture next_url %}{{ collection.next_product}}#content{% endcapture %}
                      <span class="right">
                        <a href="{{ next_url }}"> <i class="mdi mdi-chevron-right"></i></a>
                      </span>
                    {% endif %}
                  </p>
                {% endif %}
              {% endif %}
            </div>
            {% if section.settings.product_layout == 'design_3' %}
              <div class="product-single__header col-xs-6 demo">
            {% endif %}
            <p class="visually-hidden" data-product-status aria-live="polite" role="status"></p>
            <p class="visually-hidden" data-loader-status aria-live="assertive" role="alert" aria-hidden="true">
              {{ 'products.product.loader_label' | t }}
            </p>
            <div class="product-single__desc rte col-xs-12 padding_0" itemprop="description">
              {{ product.description | strip_html | truncatewords: 15 }}
            </div>
            <hr class="hr">
            {% if settings.collections_show_rating %}
              {%- for block in section.blocks -%}
                {%- case block.type -%}
                  {%- when '@app' -%}
                    {% render block %}
                {% endcase %}
              {% endfor %}
              <hr class="hr">
            {% endif %}
            {% assign my_fields = product.metafields.my_fields %}
            {% assign key = 'countdown' %}
            {% if my_fields.countdown != null %}
              <div class="flip-countdown simple-countdown">
                <div
                  class="countdown-container flip-countdown"
                  data-countdown="{{ product.metafields.my_fields.countdown }}"
                ></div>
              </div>
            {% endif %}
            {% if section.settings.product_layout != 'design_3' %}
              <div class="h2 product-single__price" id="price-{{ section.id }}">
                {% render 'product-price',
                  product: product,
                  use_variant: true,
                  show_vendor: section.settings.show_vendor
                %}
              </div>
            {% endif %}
            <div class="product-info">
              {% if section.settings.product_vendor_enable %}
                <p class="product-single__vendor">
                  <label>{{ 'products.product.product_vendor' | t }} </label
                  ><span>{{ product.vendor | link_to_vendor }}</span>
                </p>
              {% endif %}
              <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
              <meta itemprop="image" content="{{ product.featured_media.src | img_url: 'grande' }}">
              {% if collection.handle %}
                <p class="product-type">
                  <label>{{ 'products.product.product_type' | t }}</label>
                  <span
                    ><a href="{{ collection.url }}" class="collection-title text-center" item-prop="collection">
                      {{- collection.title -}}
                    </a></span
                  >
                </p>
              {% endif %}
              {% if current_variant.sku != '' or current_variant.sku != empty %}
                <p class="product-sku">
                  <label class="sku_wrapper">{{ 'customer.order.sku' | t }} : </label>
                  <span class="sku variant-sku" id="product-sku" itemprop="sku">{{ current_variant.sku }}</span>
                </p>
              {% endif %}
              {% if section.settings.product_availibility_enable %}
                <p class="product-inventory">
                  <label>{{ 'products.product.availability' | t }} </label>
                  {% if product.selected_or_first_available_variant.inventory_management %}
                    <span class="inv {% if current_variant.inventory_quantity > 0 %} instock {% else %} outstock{% endif %}">
                      <span class="items">{{ current_variant.inventory_quantity }}</span>
                      <span class="stock"> {{ 'products.product.in_stock' | t }}</span>
                      <span class="outstock"> {{ 'products.product.out_of_stock' | t }}</span>
                    </span>
                    {% for variant in product.variants %}
                      <span class="hide" id="pvr-{{ variant.id }}" data-id="{{variant.inventory_quantity}}">
                        {{- variant.inventory_quantity -}}
                      </span>
                    {% endfor %}
                  {% endif %}
                </p>
              {% endif %}
            </div>
            {% if section.settings.product_layout == 'design_3' %}
              <hr class="hr">
              {% if section.settings.share_enable %}
                {% capture permalinkURL %}{{ shop.url }}{{ product.url }}{% endcapture %}
                {% capture shareTitle %}{{ product.title | url_param_escape }}{% endcapture %}

                <div class="social-sharing" data-permalink="{{ permalinkURL }}">
                  {% if section.settings.pro_facebook %}
                    <a target="_blank" href="//www.facebook.com/sharer.php?u={{ permalinkURL }}" class="share-facebook">
                      <i class="mdi mdi-facebook"></i>
                      <span class="share-title" aria-hidden="true">{{ 'general.social.share_on_facebook' | t }}</span>
                      <span class="visuallyhidden">{{ 'general.social.alt_text.share_on_facebook' | t }}</span>
                    </a>
                  {% endif %}

                  {% if section.settings.pro_twitter %}
                    <a
                      target="_blank"
                      href="//twitter.com/share?text={{ shareTitle }}&amp;url={{ permalinkURL }}"
                      class="share-twitter"
                    >
                      <i class="mdi mdi-twitter">
  <svg class="icon icon-twitter" viewBox="0 0 20 20" style="width: 15px;height: auto;margin-top: 7px;align-items: center;justify-content: center;display: inline-block;">
<path fill="currentColor" fill-rule="evenodd" d="M7.273 2.8 10.8 7.822 15.218 2.8h1.768l-5.4 6.139 5.799 8.254h-4.658l-3.73-5.31-4.671 5.31H2.558l5.654-6.427L2.615 2.8zm6.242 13.125L5.07 4.109h1.405l8.446 11.816z" clip-rule="evenodd"></path>
</svg>
  </i> 

                      <span class="share-title" aria-hidden="true">{{ 'general.social.share_on_twitter' | t }}</span>
                      <span class="visuallyhidden">{{ 'general.social.alt_text.share_on_twitter' | t }}</span>
                    </a>
                  {% endif %}

                  {% if section.settings.pro_linkedin %}
                    <a
                      target="_blank"
                      href="https://linkedin.com/shareArticle?mini=true&amp;url={{ permalinkURL }}"
                      class="share-linkedin"
                    >
                      <i class="mdi mdi-linkedin"></i>
                    </a>
                  {% endif %}

                  {% if section.settings.pro_pinterest %}
                    <a
                      target="_blank"
                      href="//pinterest.com/pin/create/button/?url={{ permalinkURL }}&amp;media=http:{{ product.featured_image | img_url: '1024x1024' }}&amp;description={{ shareTitle }}"
                      class="share-pinterest"
                    >
                      <i class="mdi mdi-pinterest"></i>
                      <span class="share-title" aria-hidden="true">{{ 'general.social.share_on_pinterest' | t }}</span>
                      <span class="visuallyhidden">{{ 'general.social.alt_text.share_on_pinterest' | t }}</span>
                    </a>
                  {% endif %}
                  {% if section.settings.pro_tumblr %}
                    <a
                      target="_blank"
                      href="https://tumblr.com/share/link?url={{ permalinkURL }}"
                      class="share-linkedin"
                    >
                      <i class="mdi mdi-tumblr"></i>
                    </a>
                  {% endif %}
                  {% if section.settings.pro_email %}
                    <a target="_blank" href="mailto:?subject={{ permalinkURL }}" class="share-linkedin">
                      <i class="mdi mdi-email"></i>
                    </a>
                  {% endif %}
                  {% if section.settings.pro_whatsapp %}
                    <a target="_blank" href="whatsapp://send?text={{ permalinkURL }}" class="share-whatsapp">
                      <i class="mdi mdi-whatsapp"></i>
                      <span class="share-title" aria-hidden="true">{{ 'general.social.share_on_whatsapp' | t }}</span>
                      <span class="visuallyhidden">{{ 'general.social.alt_text.share_on_whatsapp' | t }}</span>
                    </a>
                  {% endif %}
                </div>
              {% endif %}
            {% endif %}
            {% if section.settings.product_layout == 'design_3' %}
              </div>
            {% endif %}

            {% if section.settings.product_layout == 'design_3' %}
              <div
                class="col-xs-6 cart_wrap content-right demo"
                itemprop="offers"
                itemscope
                itemtype="http://schema.org/Offer"
              >
                <div class="h2 product-single__price" id="price-{{ section.id }}">
                  {% render 'product-price',
                    product: product,
                    use_variant: true,
                    show_vendor: section.settings.show_vendor
                  %}
                </div>
                <div {{ block.shopify_attributes }}>
                  {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                  {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                    <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                    {{ form | payment_terms }}
                  {%- endform -%}
                </div>
            {% endif %}
            {% assign variant = product.selected_or_first_available_variant %}
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'variant_picker' -%}
                  {%- assign product_form_id = 'product-form-' | append: section.id -%}
                  {%- unless product.has_only_default_variant -%}
                    {%- if block.settings.picker_type == 'button' -%}
                      <variant-radios
                        id="variant-radios-{{ section.id }}"
                        class="no-js-hidden"
                        data-section="{{ section.id }}"
                        data-url="{{ product.url }}"
                        {% if update_url == false %}
                          data-update-url="false"
                        {% endif %}
                        {{ block.shopify_attributes }}
                      >
                        {%- for option in product.options_with_values -%}
                          <fieldset class="js product-form__input">
                            <legend class="form__label">{{ option.name }}</legend>
                            {% render 'product-variant-options', product: product, option: option, block: block %}
                          </fieldset>
                        {%- endfor -%}
                        <script type="application/json">
                          {{ product.variants | json }}
                        </script>
                      </variant-radios>
                    {%- else -%}
                      <variant-selects
                        id="variant-selects-{{ section.id }}"
                        class="no-js-hidden"
                        data-section="{{ section.id }}"
                        data-url="{{ product.url }}"
                        {% if update_url == false %}
                          data-update-url="false"
                        {% endif %}
                        {{ block.shopify_attributes }}
                      >
                        {%- for option in product.options_with_values -%}
                          <div class="product-form__input product-form__input--dropdown">
                            <label class="form__label" for="Option-{{ section.id }}-{{ forloop.index0 }}">
                              {{ option.name }}
                            </label>
                            <div class="select">
                              <select
                                id="Option-{{ section.id }}-{{ forloop.index0 }}"
                                class="select__select"
                                name="options[{{ option.name | escape }}]"
                                form="{{ product_form_id }}"
                              >
                                {% render 'product-variant-options', product: product, option: option, block: block %}
                              </select>
                            </div>
                          </div>
                        {%- endfor -%}

                        <script type="application/json">
                          {{ product.variants | json }}
                        </script>
                      </variant-selects>
                    {%- endif -%}
                  {%- endunless -%}

                  <noscript class="product-form__noscript-wrapper-{{ section.id }}">
                    <div class="product-form__input{% if product.has_only_default_variant %} hidden{% endif %}">
                      <label class="form__label" for="Variants-{{ section.id }}">
                        {{- 'products.product.product_variants' | t -}}
                      </label>
                      <div class="select">
                        <select
                          name="id"
                          id="Variants-{{ section.id }}"
                          class="select__select"
                          form="{{ product_form_id }}"
                        >
                          {%- for variant in product.variants -%}
                            <option
                              {% if variant == product.selected_or_first_available_variant %}
                                selected="selected"
                              {% endif %}
                              {% if variant.available == false %}
                                disabled
                              {% endif %}
                              value="{{ variant.id }}"
                            >
                              {%- liquid
                                echo variant.title
                                echo variant.price | money | strip_html | prepend: ' - '
                                if variant.available == false
                                  echo 'products.product.sold_out' | t | prepend: ' - '
                                endif
                                if variant.quantity_rule.increment > 1
